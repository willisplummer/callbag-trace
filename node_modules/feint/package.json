{
  "name": "feint",
  "version": "1.0.1",
  "description": "Make a given function do nothing at its first call",
  "repository": "shinnn/feint",
  "author": "Shinnosuke Watanabe (https://github.com/shinnn)",
  "scripts": {
    "prebuild": "eslint --fix --format=codeframe module.js test.js",
    "build": "rollup --config=node:module",
    "pretest": "npm run-script build",
    "test": "istanbul cover test.js"
  },
  "license": "CC0-1.0",
  "module": "module.js",
  "files": [
    "index.js",
    "module.js"
  ],
  "keywords": [
    "feint",
    "function",
    "call",
    "noop",
    "time"
  ],
  "dependencies": {
    "append-type": "^1.0.0"
  },
  "devDependencies": {
    "@shinnn/eslint-config-node-legacy": "^3.0.0",
    "eslint": "^3.19.0",
    "istanbul": "^0.4.5",
    "rollup": "^0.41.6",
    "rollup-config-module": "^1.0.0",
    "tape": "^4.6.3"
  },
  "eslintConfig": {
    "extends": "@shinnn/node-legacy",
    "rules": {
      "consistent-return": "off",
      "prefer-rest-params": "off"
    }
  }
}
