{
  "name": "install-purescript-cli",
  "version": "0.2.0",
  "description": "A command-line tool to install PureScript",
  "repository": "shinnn/install-purescript-cli",
  "author": "Shinnosuke Watanabe (https://github.com/shinnn)",
  "scripts": {
    "run-docker": "docker run --tty --volume $PWD:/project --workdir /project node node . tmp/docker",
    "pretest": "eslint --fix --format=codeframe index.js test",
    "test": "nyc --reporter=html --reporter=text ava"
  },
  "license": "MIT",
  "files": [
    "index.js"
  ],
  "bin": {
    "install-purescript": "index.js"
  },
  "keywords": [
    "purescript",
    "purs",
    "binary",
    "exe",
    "executable",
    "haskell",
    "download",
    "prebuilt",
    "fallback",
    "build",
    "compile",
    "stack",
    "ghc",
    "install",
    "installation",
    "cache",
    "cross-platform",
    "command-line",
    "cli"
  ],
  "dependencies": {
    "chalk": "^2.3.0",
    "install-purescript": "^0.2.0",
    "log-symbols": "^2.1.0",
    "log-update": "^2.3.0",
    "minimist": "^1.2.0",
    "ms": "^2.0.0",
    "neat-frame": "^0.2.0",
    "neat-stack": "^0.1.1",
    "once": "^1.4.0",
    "platform-name": "^0.5.0",
    "size-rate": "^0.1.0",
    "tilde-path": "^2.0.0",
    "tty-truncate": "^0.3.1",
    "vertical-meter": "^0.1.0"
  },
  "devDependencies": {
    "@shinnn/eslint-config-cli": "^3.0.0",
    "app-cache-dir": "^0.3.0",
    "arch": "^2.1.0",
    "ava": "^0.23.0",
    "cp-file": "^5.0.0",
    "different-platform": "^1.0.0",
    "escape-string-regexp": "^1.0.5",
    "eslint": "^4.11.0",
    "execa": "^0.8.0",
    "lstat": "^1.0.0",
    "nock": "^9.1.0",
    "nyc": "^11.3.0",
    "path-exists": "^3.0.0",
    "path-key": "^2.0.1",
    "pretend-platform": "^2.0.0",
    "request": "^2.83.0",
    "rmfr": "^1.0.3",
    "strip-ansi": "^4.0.0",
    "term-size": "^1.2.0",
    "to-executable-name": "^2.0.0"
  },
  "ava": {
    "files": "test/test.js",
    "serial": true,
    "verbose": true
  },
  "eslintConfig": {
    "extends": "@shinnn/node",
    "rules": {
      "no-process-exit": "off"
    }
  }
}
